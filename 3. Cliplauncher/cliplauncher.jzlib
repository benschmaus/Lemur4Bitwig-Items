<SCRIPT name="onCursorDown(cursor,hit,x,y)" script="decl hitcounter = 0;
decl i;
for(i=0;i<multitouch_cursors;i++){
 if(cursors_alive[i] == 0){
  cursors_id[i] = cursor;
  cursors_alive[i] = 1;
  break;
 }
}

if(hitboxtype[hit] == hittype_clip){
 clipishit[hit-hittype_clip_startindex]=1;
} else if (hitboxtype[hit] == hittype_stop){
 decl basehit = hit - hittype_stop_startindex;
 decl track = basehit;
} else if (hitboxtype[hit] == hittype_playscene){
 decl basehit = hit - hittype_playscene_startindex;
 decl clip = basehit;
} else if (hitboxtype[hit] == hittype_sequencer){
 decl basehit = hit - hittype_sequencer_startindex;
 decl track = basehit;
}
" trigger_script="" trigger_type="255" trigger_name="onCursorDown" trigger="1" clock="0" clock_div="4" osc_message="/Cliplauncher/onCursorDown" midi_message="0x90,0x90,0,0" midi_target="-1" flag="1"/>
<SCRIPT name="onCursorUp(cursor,hit,x,y)" script="

decl cursor_index;
decl i;
for(i=0;i<multitouch_cursors;i++){
 if(cursors_id[i] == cursor){
  cursors_alive[i] = 0;
  cursors_id[i] = 0;
  cursor_index = i;
  break;
 }
}

decl hitcounter = 0;

if(hitboxtype[hit] == hittype_clip){
 //Make sure the cursor up event happens on the same clip that was pressed
 if(clipishit[hit-hittype_clip_startindex]){
  decl basehit = hit - hittype_clip_startindex;
  decl track = floor(basehit / numtracks + 1);
  decl clip = floor(basehit % numclips + 1);
  oscout(0,'/track/' + track + '/clip/' + clip + '/launch',{1});
 }
 clipishit[hit-hittype_clip_startindex]=0;
 
} else if (hitboxtype[hit] == hittype_stop){
 decl basehit = hit - hittype_stop_startindex;
 decl track = basehit+1;
 oscout(0,'/track/' + track + '/clip/stop',{1});
} else if (hitboxtype[hit] == hittype_playscene){
 decl basehit = hit - hittype_playscene_startindex;
 decl track = basehit+1;
 oscout(0,'/scene/' + track + '/launch',{1});
} else if (hitboxtype[hit] == hittype_sequencer){
 decl basehit = hit - hittype_sequencer_startindex;
 decl track = basehit+1;
 oscout(0,'/track/' + track + '/clip/returntoarrangement',{1});
}
