<SCRIPT name="draw_clips()" script="decl c = getobject();

decl hitbox = 0;
decl clipnum = 0;
decl startx; decl starty;
decl x; decl y;

for(x=0;x<numtracks;x++){
 for(y=0;y<numclips;y++){
  //Draw Clips
  if(canvas_rendering_optimizations && clip_redraw_list[x * numclips + y] != 1)
   continue;
  
  hitbox = hittype_clip_startindex + (numclips * x) + y;

  clipnum = hitbox - hittype_clip_startindex;

  startx = scenelauncher_width + x * clip_width;
  starty = header_height + y * clip_height;

  canvas_beginPath(c);
  canvas_rect(c,startx,starty,clip_width,clip_height);
 canvas_addHitRegion(c,0,hitbox);
  

 if(clips_exist[clipnum] == 1 ){

   canvas_setFillStyle(c,{clip_r[clipnum],clip_g[clipnum],clip_b[clipnum]});
   canvas_fill(c);
   canvas_stroke(c);

   canvas_setFillStyle(c,{0});
   canvas_fillText(c,clipnames[clipnum],startx + clip_width/2,starty + clip_height/(numclips/2),clip_width - clip_width/numtracks);

  } else{
   canvas_setFillStyle(c,color_emptyclip);
   canvas_fill(c);
   canvas_stroke(c);
  }

 clip_redraw_list[clipnum] = 0;

 }

}" trigger_script="" trigger_type="4" trigger="1" clock="0" clock_div="4" osc_message="/Cliplauncher/draw_clips" midi_message="0x90,0x90,0,0" midi_target="-1" flag="1"/>
<SCRIPT name="draw_tracks()" script="decl c = getobject();

decl x;
decl startx; decl starty;

decl hitbox;

for(x=0;x<numtracks;x++){

 if(canvas_rendering_optimizations==1 && track_redraw_list[x] != 1)
  continue;

 //Draw Track Header
 startx = scenelauncher_width + x * clip_width;
 starty = 0; 

 canvas_beginPath(c);
 
 if(track_exist[x]){

  canvas_setFillStyle(c,color_emptytrack);

  canvas_rect(c,startx,starty,clip_width,header_height); 
  canvas_fill(c);
  canvas_stroke(c); 

  canvas_setFillStyle(c,{track_r[x],track_g[x],track_b[x]});

  canvas_fillText(c,tracknames[x],startx + clip_width/2,starty + clip_height/4,clip_width - clip_width/numtracks);

 }else{
  canvas_setFillStyle(c,color_emptytrack);
  canvas_rect(c,startx,starty,clip_width,header_height); 
  canvas_fill(c);
  canvas_stroke(c); 
 }

 //Draw Stop
 canvas_beginPath(c);
 startx = scenelauncher_width + (x* clip_width);
 starty = (clip_height * numclips) + header_height;

 if(track_playing[x] || track_recording[x]) canvas_setFillStyle(c,color_emptytrack);
 else canvas_setFillStyle(c,color_emptytrack);

 canvas_rect(c,startx,starty,clip_width/2,canvas_height - starty); 
 canvas_fill(c);
 canvas_stroke(c); 

 hitbox = hittype_stop_startindex + x;
 canvas_addHitRegion(c,0,hitbox);

 canvas_beginPath(c);

 if(track_playing[x] || track_recording[x]) canvas_setFillStyle(c,color_tracktext);
 else canvas_setFillStyle(c,{color_tracktext[0]/4,color_tracktext[1]/4,color_tracktext[2]/4});

 canvas_rect(c,startx + clip_width/8,starty + clip_height/8,clip_width/4, clip_height/4);
 canvas_fill(c);
 canvas_stroke(c);
 canvas_closePath(c);

 //Draw Sequencer Buttons
 canvas_beginPath(c);
 startx = scenelauncher_width + (x* clip_width) + clip_width/2;
 starty = (clip_height * numclips) + header_height;

 canvas_setFillStyle(c,color_emptytrack);

 canvas_rect(c,startx,starty,clip_width/2,footer_height); 
 canvas_fill(c);
 canvas_stroke(c); 
 hitbox = hittype_sequencer_startindex + x;
 canvas_addHitRegion(c,0,hitbox);

 decl i;
 for(i = 0; i<3; i++){
  canvas_beginPath(c);

  if(track_arranger[x]) canvas_setFillStyle(c,color_tracktext);
  else canvas_setFillStyle(c,{color_tracktext[0]/4,color_tracktext[1]/4,color_tracktext[2]/4});

  canvas_rect(c,startx + clip_width/8,(starty + clip_height/8)+((clip_height/10)*i),clip_width/4, clip_height/12);
  canvas_fill(c);
  canvas_stroke(c);
  canvas_closePath(c);
 }
 track_redraw_list[x] = 0;
}" trigger_script="" trigger_type="4" trigger="1" clock="0" clock_div="4" osc_message="/Cliplauncher/draw_tracks" midi_message="0x90,0x90,0,0" midi_target="-1" flag="1"/>
<SCRIPT name="draw_scenelauncher()" script="decl c = getobject();

decl startx; decl starty;
decl y;

//Draw Scene Launcher & Stop Buttons
for(y=0;y<numclips;y++){
 if(canvas_rendering_optimizations && scenelauncher_redraw_list[y] != 1)
  continue;

 //Draw Scene Launcher
 canvas_beginPath(c);
 startx = 0;
 starty = (clip_height * y) + header_height; 

 canvas_setFillStyle(c,color_emptytrack);
 canvas_rect(c,startx,starty,scenelauncher_width,clip_height); 
 canvas_fill(c);
 canvas_stroke(c);
 canvas_addHitRegion(c,0,hittype_playscene_startindex + y);

 
 canvas_setFillStyle(c,color_tracktext);
 canvas_fillText(c,scenenames[y],startx + clip_width/2,starty + clip_height/(numclips/2),clip_width - clip_width/numtracks);

 canvas_beginPath(c);
 canvas_setFillStyle(c,color_tracktext);
 canvas_moveTo(c,startx + clip_width/2.8 , starty + clip_height/2.5 +5 );
 canvas_lineTo(c, startx + clip_width/2.8 , starty + clip_height/1.25  +5);
 canvas_lineTo(c, startx + clip_width/1.4 , starty + clip_height/1.875 +5 );
 canvas_stroke(c);
 canvas_fill(c);
 
 
}" trigger_script="" trigger_type="4" trigger="1" clock="0" clock_div="4" osc_message="/Cliplauncher/draw_scenelauncher" midi_message="0x90,0x90,0,0" midi_target="-1" flag="1"/>
<SCRIPT name="draw()" script=" if(canvas_rendering_optimizations == 1 && redraw_needed == 0)
   return;

 if(!configured){
  configuration();
 }

 decl c = getobject();
 decl rect = getattribute(c, 'rect');
 canvas_width = rect[2]; canvas_height = rect[3]; 

 //Setup constant canvas drawing styles
 canvas_setLineWidth(c,1);
 canvas_setStrokeStyle(c,color_border);
 canvas_setFontSize(c,16);
 canvas_setTextAlign(c, 1);
 canvas_setTextBaseline(c, 1); 

 draw_clips();
 draw_tracks();
 draw_scenelauncher();

 redraw_needed = 0;

" trigger_script="" trigger_type="255" trigger_name="onRedraw" trigger="1" clock="0" clock_div="4" osc_message="/Cliplauncher/draw" midi_message="0x90,0x90,0,0" midi_target="-1" flag="1"/>
<SCRIPT name="configuration()" script="//Cliplauncher configuration
numclips = 8;
numtracks = 8;
scenelauncher_width = canvas_width / numtracks;
header_height = (canvas_height / numclips / 2);
footer_height = (canvas_height / numclips / 2);

